/dts-v1/;

/include/ "msmzinc.dtsi"

/ {
	model = "Qualcomm MSM ZINC Simulator";
	compatible = "qcom,msmzinc-sim", "qcom,msmzinc", "qcom,sim";
	qcom,msm-id = <178 0 0>;

	aliases {
		serial0 = &uart0;
	};

	uart0: serial@f991f000 {
		status = "ok";
	};
};
